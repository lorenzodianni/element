    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2015-rollup": "^1.2.0",
    "del-cli": "^0.2.0",
    "eslint": "^3.11.1",
    "eslint-config-google": "^0.7.1",
    "ghooks": "^1.3.2",
    "jasmine-core": "^2.5.2",
