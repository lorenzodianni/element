{
  "name": "elent",
  "version": "0.1.0",
  "description": "functional library for element composition",
  "main": "index.js",
  "dependencies": {
    "rollup": "^0.36.4"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015-rollup": "^1.2.0",
    "del-cli": "^0.2.0",
    "eslint": "^3.11.1",
    "eslint-config-google": "^0.7.1",
    "rollup": "^0.36.4",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-commonjs": "^5.0.5",
    "rollup-plugin-node-resolve": "^2.0.0",
    "uglify-js": "^2.7.5"
  },
  "scripts": {
    "rollup": "./node_modules/.bin/rollup",
    "uglifyjs": "./node_modules/.bin/uglifyjs",
    "del": "./node_modules/.bin/del",
    "clean": "npm run del dist -f",
    "bundle": "npm run rollup -- -c",
    "minify": "npm run uglifyjs -- --c --m -o dist/elent.min.js -- dist/elent.js",
    "dist": "npm run clean && npm run bundle && npm run minify"
  },
